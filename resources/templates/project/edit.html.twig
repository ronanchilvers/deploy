{% extends 'page.html.twig' %}

{% block javascripts %}
<script type="text/javascript">
App.Deploy.init({
    project: '{{ project.key }}'
});
</script>
{% endblock %}

{% block body %}
<div class="project">

    <div class="header">
        {{ include('fragments/project/header.html.twig', {
            project: project,
            show_button_backlink: true,
        } )}}
    </div>

    {{ include('fragments/project/tabs.html.twig', {
        project: project,
        active: 'project.edit',
    } )}}

    <section class="section">
        {# <div class="box"> #}
            {% include 'fragments/project/form.html.twig' %}
        {# </div> #}
    </section>

</div>
{% endblock %}
